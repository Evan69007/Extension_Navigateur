{
	"manifest_version": 3,
	"name": "Orbit",
	"version": "2.0",
	"permissions":[
		"bookmarks",
		"activeTab",
		"tabs"
	],
    "host_permissions": [
    	"http://*/*",
    	"https://*/*"
    ],
    "action": {
        "default_title": "Open Hub",
		"default_icon": "OrbitIcon.png"
    },
    "background": {
       "service_worker": "background.js"
    }  
}