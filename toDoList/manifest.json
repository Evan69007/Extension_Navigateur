{
    "manifest_version": 3,
    "name": " Minimalist To do List",
    "description": "Embed To do List in your browser",
    "version": "0.0.1",
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "host_permissions": [
    "http://*/*",
    "https://*/*"
    ],
    "action": {
        "default_title": "To Do",
        "default_icon": "check-mark-symbol-transparent-background-2.png"
      },
    "background": {
       "service_worker": "background.js"
    }  
      

}